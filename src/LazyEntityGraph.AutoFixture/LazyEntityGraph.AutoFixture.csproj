<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net452</TargetFrameworks>
    <PackageId>LazyEntityGraph.AutoFixture</PackageId>
    <Product>LazyEntityGraph.AutoFixture</Product>
    <Authors>dbroudy,alexfoxgill</Authors>
    <Company>dbroudy</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/dbroudy/LazyEntityGraph/blob/master/license.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/dbroudy/LazyEntityGraph</PackageProjectUrl>
    <Description>An AutoFixture customization for lazy-loading entities using LazyEntityGraph</Description>
    <PackageReleaseNotes>Update AutoFixture to version 4 namespace</PackageReleaseNotes>
    <PackageTags>LazyEntityGraph AutoFixture</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/dbroudy/LazyEntityGraph</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoFixture" Version="4.3.0" />
    <PackageReference Include="Fare" Version="2.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\LazyEntityGraph.Core\LazyEntityGraph.Core.csproj" />
  </ItemGroup>

</Project>